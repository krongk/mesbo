<div class="main_corner">
  <%=image_tag asset_path("show_06.jpg"), :width =>'660px' %>
  <div class="content">
    <h2 style='font-size: 24px; font-weight: 600; padding: 20px 0; border-bottom: dotted 2px #333;'>
      洗衣行业分类信息汇总
    </h2>
    
    <table>
    <% @note_cates.each do |note_cate| %>
      <tr>
        <td><%= note_cate.name %></td>
        <td><%= link_to 'Show', note_cate %></td>
        <td><%= link_to 'Edit', edit_note_cate_path(note_cate) %></td>
        <td><%= link_to 'Destroy', note_cate, confirm: 'Are you sure?', method: :delete %></td>
      </tr>
    <% end %>
    </table>
  
     
  </div>
   <%=image_tag asset_path("show_08.jpg"), :width =>'660px' %>
</div>


